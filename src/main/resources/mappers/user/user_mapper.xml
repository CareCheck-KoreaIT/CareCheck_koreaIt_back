<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.korit.carecheckkoreait.mapper.UserMapper">
    <insert id="insert" useGeneratedKeys="true" keyProperty="userId">
        insert into user_tb
        values (
            default,
            #{password},
            #{username},
            #{email},
            #{phoneNumber},
            #{accountExpired},
            #{accountLocked},
            #{credentialsExpired},
            #{accountEnabled},
            now(),
            now()
    </insert>


    <select id="selectById" resultType="com.korit.carecheckkoreait.entity.User">

    </select>
</mapper>